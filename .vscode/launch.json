{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome",
      "url": "http://localhost:3010",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "node",
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "server",
      "program": "${workspaceFolder}/server.js",
      "args": ["--env.app", "bulutcrm"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "ts-node",
      "args": ["${relativeFile}"],
      "runtimeArgs": ["-r", "ts-node/register"],
      "cwd": "${workspaceRoot}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "jasmine-ts",
      "program": "${workspaceFolder}/node_modules/jasmine-ts/lib/index.js",
      "args": [
        "/Users/macbook/DEV/bulutcrm/bulutcrm-repo/app-packages/babel-plugin-transform-fjsx-syntax/test/index.ts"
      ]
    }
  ]
}
